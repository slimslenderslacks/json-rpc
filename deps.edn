  com.cognitect/transit-clj {:mvn/version "1.0.329"}
  com.rpl/specter {:mvn/version "1.1.4"}
  org.flatland/ordered {:mvn/version "1.15.10"}
  io.replikativ/hasch {:mvn/version "0.3.7"}
