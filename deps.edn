  org.clj-commons/clj-http-lite {:mvn/version "1.0.13"}
  org.clj-commons/hickory {:mvn/version "0.7.3"}
  babashka/process {:mvn/version "0.2.9"}
  babashka/babashka.pods {:mvn/version "0.1.0"}

   ;; for the code in src/clojure_lsp
  borkdude/dynaload {:mvn/version "0.3.5"}
  funcool/promesa {:mvn/version "9.0.470"}
  com.taoensso/timbre {:mvn/version "5.2.1"}
  com.github.clojure-lsp/clojure-lsp {:mvn/version "2023.08.06-00.28.06"}
  io.github.atomisthq/datalog-schema #_{:local/root "/Users/slim/atmhq/datalog_schema"}
  {:git/url "https://github.com/atomisthq/datalog-schema.git"
   :git/sha "bc5ecf42db7bbdb058ac36f9f386fb4a89db6c36"}

  clj-commons/clj-yaml {:mvn/version "1.0.27"}

   ;io.github.atomisthq/datoms
   ;{:local/root "/Users/slim/atmhq/datoms"}
